{
  "name": "common-frontend",
  "version": "2.0.3",
  "description": "",
  "main": "./lib",
  "author": "Pemberton Rank Ltd",
  "homepage": "https://bitbucket.org/pembertonrank/common-frontend#readme",
  "license": "MIT",
  "scripts": {
    "prepublish": "webpack --mode=build",
    "build": "webpack --watch",
    "babelbuild": "babel ./src --out-dir ./lib --source-maps --presets es2015,react --plugins babel-plugin-add-module-exports,babel-plugin-transform-decorators-legacy,babel-plugin-transform-class-properties --watch",
    "start": "webpack-dev-server --progress --colors --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pemberton-rank/common-frontend.git"
  },
  "babel": {
    "compact": "false",
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "React"
    ]
  },
  "peerDependencies": {
    "alt": "^0.18.5",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "material-ui": "^0.15.4",
    "axios": "^0.13.1",
    "react-router": "^2.6.1",
    "moment": "^2.17.1",
    "formsy-material-ui": "^0.5.5",
    "formsy-react": "^0.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "browserify": "^13.1.0",
    "browserify-global-shim": "^1.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "browserify-global-shim": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "dependencies": {
    "alt-utils": "^1.0.0",
    "material-ui": "^0.15.4",
    "moment": "^2.17.1"
  }
}
